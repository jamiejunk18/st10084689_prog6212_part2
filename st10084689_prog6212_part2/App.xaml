<Application x:Class="st10084689_prog6212_part2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:st10084689_prog6212_part2"
             xmlns:view="clr-namespace:st10084689_prog6212_part2.MVVM.View"
             xmlns:vm="clr-namespace:st10084689_prog6212_part2.MVVM.ViewModel" 
             StartupUri="MVVM/View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:SignInViewModel}">
                <view:SignInView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:LogInViewModel}">
                <view:LogIn/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
